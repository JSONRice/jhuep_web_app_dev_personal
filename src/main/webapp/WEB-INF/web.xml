<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" 
         xmlns="http://java.sun.com/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>jhuep_web_app_dev_personal</display-name>
<!--
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>   
-->
    <servlet>
        <servlet-name>ComputeCostController</servlet-name>
        <servlet-class>resources.servlets.ComputeCostController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfirmationController</servlet-name>
        <servlet-class>resources.servlets.ConfirmationController</servlet-class>                
    </servlet>
    <servlet-mapping>
        <servlet-name>ComputeCostController</servlet-name>
        <url-pattern>/ComputeCostController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmationController</servlet-name>
        <url-pattern>/ConfirmationController</url-pattern>        
    </servlet-mapping>    
<!--
    <error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/error.jsp</location>
    </error-page>
-->
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
